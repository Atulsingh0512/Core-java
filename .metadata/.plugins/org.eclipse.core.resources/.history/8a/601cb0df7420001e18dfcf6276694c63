package thiskeyword;


//this is used to refer current class method..

class Student{
	int rollno;
	String name;
	float fee;
	
	public Student(int rollno, String name, float fee) {
		super();
		this.rollno = rollno;
		this.name = name;
		this.fee = fee;
	}
	
	void display()
	{
		System.out.println("rollno is"+rollno+  "name is"+name+ "fees is"+fee );
	}

}

public class TestThis2 {

	public static void main(String[] args) {
		Student s1=new Student(1,"Atul",150000);
		Student s2=new Student(2,"vipul",200000);
		s1.display();
		s2.display();
		
	}

}
