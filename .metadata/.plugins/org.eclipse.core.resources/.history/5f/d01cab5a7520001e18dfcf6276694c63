package thiskeyword;


//this is used to refer current class method..

class Student1{
	int rollno;
	String name;
	float fee;
	
	public Student1(int rollno, String name, float fee) {
		super();
		this.rollno = rollno;
		this.name = name;
		this.fee = fee;
	}
	
	void display()
	{
		System.out.println("rollno is "+ rollno +  "name is "+ name+ " fees is"+ fee );
	}

}

public class TestThis2 {

	public static void main(String[] args) {
		Student1 s1=new Student1(1,"Atul",150000);
		Student1 s2=new Student1(2,"vipul",200000);
		s1.display();
		s2.display();
		
	}

}
